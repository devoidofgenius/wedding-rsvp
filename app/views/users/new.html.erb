<% provide(:title, 'Sign up') %>

<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
        <h1>Sign up</h1>
        <%= simple_form_for(@user, html: { class: 'form-horizontal' }) do |f| %>
          <%= render 'fields', f: f %>
          <%= f.button :submit, "Create my account", class: "btn btn-primary" %>
        <% end %>
    </div>
  </div>
</div>
<%= debug(params) if Rails.env.development? %>